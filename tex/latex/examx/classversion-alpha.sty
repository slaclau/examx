\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{classversion-alpha}[2022/02/03 Used for 
versioning classes]
\RequirePackage{xparse}
\RequirePackage{pgf}
\RequirePackage{readprov}

\newcounter{classversion@releasenumber}
\setcounter{classversion@releasenumber}{0}

\def\classversion@releases{}

\NewDocumentCommand{\NewRelease}{m}{
    \addtocounter{classversion@releasenumber}{1}
    \gdef\classversion@currentRelease{#1}
    \xdef\classversion@releases{\classversion@releases,#1}
    \xdef\temp{{\classversion@releases}}
    \begin{tikzpicture}\temp[1];\end{tikzpicture}
}
